{
  "name": "tootr",
  "version": "0.0.1",
  "description": "Toot your goings on",
  "repository": {
    "type": "git",
    "url": "git://github.com/lmorchard/tootr.git"
  },
  "scripts": {
    "server-watch": "./node_modules/.bin/supervisor src/javascript/server.js",
    "start": "node src/javascript/server.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browser": {
    "timeago": "./src/javascript/vendor/jquery.timeago.js",
    "S3Ajax": "./src/javascript/vendor/S3Ajax.js",
    "MD5": "./src/javascript/vendor/md5.js"
  },
  "browserify-shim": {
    "jquery": "global:$",
    "S3Ajax": {
      "exports": "S3Ajax"
    },
    "MD5": "MD5"
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "coffeeify": "~0.7.0",
    "gulp": "^3.8.7",
    "gulp-awspublish": "0.0.23",
    "gulp-changed": "^0.4.1",
    "gulp-gh-pages": "^0.4.0",
    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-s3": "~0.3.0",
    "gulp-stylus": "^1.3.3",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "~2.0.0",
    "nib": "^1.0.4",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "stylus": "^0.49.1",
    "supervisor": "~0.6.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~1.0.2",
    "gulp-connect": "~2.0.6"
  },
  "dependencies": {
    "async": "^0.9.0",
    "backbone": "~1.1.2",
    "body-parser": "^1.9.0",
    "cors": "~2.5.0",
    "express": "~3.16.0",
    "jquery": "~2.1.0",
    "knox": "~0.9.1",
    "microformat-shiv": "git://github.com/glennjones/microformat-shiv",
    "pubsub-js": "~1.5.0",
    "request": "^2.45.0",
    "underscore": "^1.7.0"
  }
}
